package String;

public class Upper_to_lower_viceversa 
{
	public static void main(String[] args)
	{
		String str="GururaJ";
		char[] str1=str.toCharArray();
		char ch;
		int len=str1.length;
		
		for(int i=0;i<len;i++)
		{
			if(ch >= 'a' && ch <= 'z')
			{
				str1[i] = str1(i) - 32;
			}
			else
			{
				str1[i] = str1(i) + 32;
			}
		}
		System.out.println(str1);
	}
}
