package Inheritence;

class parent
{
	public void p1()
	{
		System.out.println("Parent method");
	}
}
public class inheritence extends parent
{
	public void c1()
	{
		System.out.println("Child method");
	}
	public static void main(String[] args)
	{
		inheritence obj=new inheritence();
		obj.p1();
		obj.c1();
	}
}