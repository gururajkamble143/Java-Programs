package String;

public class Reverse_string 
{
	public static void main(String[] args)
	{
		String str="GURURAJ";
		String reverse=" ", word=" ";
		
		str.length();
		
		System.out.print("lenght = "+str.length());
		
		for(int i=str.length();i>=0;i--)
		{
			System.out.println(str.charAt(i));
		}
		
	}
}
