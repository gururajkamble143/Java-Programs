package Inheritence;

class base
{
	public void display()
	{
		System.out.println("It is a base class");
	}
}

public class Derived extends base
{
	public void display()
	{
		super.display();
		System.out.println("It is a derived class");
	}
	public static void main(String[] args)
	{
		Derived d1=new Derived();
		d1.display();
	}
}
