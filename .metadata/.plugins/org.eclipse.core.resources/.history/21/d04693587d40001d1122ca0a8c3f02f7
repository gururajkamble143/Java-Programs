package String;

public class String_comparison 
{
	public static void main(String[] args)
	{
		String s="Hell";
		String s1="Hello";
		String s2="Hello";
		
		if(s1.equals(s2))
		{
			System.out.println("True");
		}
		else
		{
			System.out.println("False");
		}
		if(s.equals(s1))
		{
			System.out.println("True");
		}
		else
		{
			System.out.println("False");
		}
	}
}
