package String;

public class Reverse_string 
{
	public static void main(String[] args)
	{
		String str="GURURAJ";
		String word=" ";
		char ch;
		str.length();
		
		System.out.print("lenght = "+str.length());
		
		for(int i=str.length();i>=0;i--)
		{
			ch=str.charAt(i);
			word=ch+word;
		}
		System.out.println("Reverse word = "+word);
		
	}
}
