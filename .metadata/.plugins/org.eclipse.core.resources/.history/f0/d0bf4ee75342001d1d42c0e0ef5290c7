package String;

public class Upper_to_lower_viceversa 
{
	public static void main(String[] args)
	{
		String str="GururaJ";
		char[] str1=str.toCharArray();
		int len=str1.length;

		for(int i=0;i<len;i++)
		{
			//ch=str.charAt(i);
			if(str1[i] >= 'a' && str1[i] <= 'z')
			{
				str1[i] = (char)((int)str1[i]-32) ;
			}
			//str1 += (char)ch;
		}
		System.out.println(str1);
	}
}
