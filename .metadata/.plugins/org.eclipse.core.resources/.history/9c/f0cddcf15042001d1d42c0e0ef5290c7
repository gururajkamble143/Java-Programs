package String;

public class Upper_to_lower_viceversa 
{
	public static void main(String[] args)
	{
		String str="GururaJ";
		String str1="";
		int len=str.length();
		
		for(int i=0;i<len;i++)
		{
			if(char>='a' && char<='z')
			{
				str1=str-32;
			}
			else
			{
				str1=str+32;
			}
		}
		System.out.println(str1);
	}
}
