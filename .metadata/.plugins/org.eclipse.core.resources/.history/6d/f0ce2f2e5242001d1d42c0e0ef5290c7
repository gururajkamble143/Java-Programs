package String;

public class Upper_to_lower_viceversa 
{
	public static void main(String[] args)
	{
		String str="GururaJ";
		char[] str1=str.toCharArray();
		int len=str1.length;
		int ch;
		for(int i=0;i<len;i++)
		{
			ch=str.charAt(i);
			if(ch >= 'a' && ch <= 'z')
			{
				ch = ch - 32;
			}
			else
			{
				ch = ch + 32;
			}
		}
		System.out.println(str1);
	}
}
